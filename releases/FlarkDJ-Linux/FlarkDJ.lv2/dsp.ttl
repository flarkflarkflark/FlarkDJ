plug:filterEnabled
	a lv2:Parameter ;
	rdfs:label "Filter Enabled" ;
	rdfs:range atom:Float ;
	lv2:default 1 ;
	lv2:minimum 0 ;
	lv2:maximum 1	 ;
	lv2:portProperty lv2:enumeration , lv2:toggled ;
	lv2:scalePoint [
		rdfs:label "Off" ;
		rdf:value 0 ;
	], [
		rdfs:label "On" ;
		rdf:value 1 ;
	] .

